import pandas as pd
import os

# Load both files from Desktop
desktop = os.path.join(os.path.expanduser("~"), "Desktop")
file_a_path = os.path.join(desktop, "A.xlsx")
file_b_path = os.path.join(desktop, "B.xlsx")

# Read Excel files
df_a = pd.read_excel(file_a_path)
df_b = pd.read_excel(file_b_path)

# Ensure 'Test' column exists
if 'Test' not in df_a.columns or 'Test' not in df_b.columns:
    raise ValueError("Column 'Test' not found in one or both files.")

# Clean and normalize (trim + lowercase for accurate match)
a_test = df_a['Test'].astype(str).str.strip().str.lower()
b_test = df_b['Test'].astype(str).str.strip().str.lower()

# Get unique values in B not in A
diff_values = sorted(set(b_test) - set(a_test))

# Print count and values
print(f"ðŸ§® Count of 'Test' values in B but not in A: {len(diff_values)}\n")

for val in diff_values:
    print(val)
