# --- RE-APPLY Jurisdiction dropdown after sync (py311 table reset) ---
col_props = self.AtomComplexGrid().ColumnProperties()
jur = col_props.get('Jurisdiction')
if isinstance(jur, dict) and 'EditorSettings' in jur:
    ed = jur['EditorSettings']

    # Only (re)apply if Items look empty or missing
    have = (ed.get('Items') or ed.get('Options') or ed.get('List') or [])
    if not have:
        items = ["Argentina", "Australia", "Austria", "Belgium"]
        selected = ["Australia", "Belgium"]  # multi-select expects list[str]

        # write to all plausible keys (covers py39/py311)
        for k in ("Items", "items", "Options", "options", "List", "list"):
            ed[k] = items
        for k in ("value", "Value", "selected"):
            ed[k] = selected

        print("Re-applied JUR Items:", items, "| selected:", selected)
# --- end re-apply ---
