# --- Re-apply Jurisdiction dropdown after sync ---
col_props = self.AtomComplexGrid().ColumnProperties()
if "Jurisdiction" in col_props and "EditorSettings" in col_props["Jurisdiction"]:
    ed = col_props["Jurisdiction"]["EditorSettings"]

    # hardcoded fallback values for testing
    hardcoded_items = ["Argentina", "Australia", "Austria", "Belgium"]
    hardcoded_selected = ["Australia", "Belgium"]

    # re-apply for all plausible keys
    for k in ("Items", "items", "Options", "options", "List", "list"):
        ed[k] = hardcoded_items
    for k in ("value", "Value", "selected"):
        ed[k] = hardcoded_selected

    print("✅ Re-applied Jurisdiction Items:", ed.get("Items") or [])
    print("✅ Re-applied Jurisdiction Selected:", ed.get("value") or ed.get("selected"))
# --- end re-apply ---
