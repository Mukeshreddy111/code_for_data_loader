def onCubeInitialized(self, cubeRes):
    ...
    self.AtomComplexGrid().OnTableAvailable(self._rebind_jurisdiction_editor)

def _rebind_jurisdiction_editor(self, *args, **kwargs):
    props = self.AtomComplexGrid().ColumnProperties()
    if "Jurisdiction" in props:
        ed = props["Jurisdiction"].setdefault("EditorSettings", {})
        props["Jurisdiction"]["EditorType"] = "checkedcomboboxedit"
        print("üîÅ Rebinding Jurisdiction editor with", len(ed.get("Items", [])), "items")
